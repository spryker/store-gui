{
    "name": "spryker/store-gui",
    "type": "library",
    "description": "StoreGui module",
    "license": "proprietary",
    "require": {
        "php": ">=8.0",
        "spryker/gui": "^3.38.0",
        "spryker/kernel": "^3.33.0",
        "spryker/store": "^1.15.0",
        "spryker/symfony": "^3.0.0",
        "spryker/transfer": "^3.25.0",
        "spryker/util-encoding": "^1.0.0 || ^2.0.0"
    },
    "require-dev": {
        "spryker/code-sniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spryker\\": "src/Spryker/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "cs-check": "phpcs -p -s --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/",
        "cs-fix": "phpcbf -p --standard=vendor/spryker/code-sniffer/Spryker/ruleset.xml src/"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true
        }
    }
}
